<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GameActivity">

    <ImageView
        android:id="@+id/goalTile"
        android:layout_width="512dp"
        android:layout_height="65dp"
        app:layout_constrainedHeight="true"
        app:layout_constrainedWidth="true"
        app:layout_constraintBottom_toTopOf="@+id/roadTile"
        app:layout_constraintStart_toStartOf="@id/safeTile4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5"
        app:srcCompat="@drawable/goal_tile" />

    <ImageView
        android:id="@+id/roadTile"
        android:layout_width="0dp"
        android:layout_height="118dp"
        android:adjustViewBounds="false"
        android:cropToPadding="false"
        android:scrollIndicators="top"
        android:translationY="-40dp"
        app:layout_constraintBottom_toTopOf="@id/roadTile2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/goalTile"
        app:layout_marginBaseline="0dp"
        app:srcCompat="@drawable/road" />

    <ImageView
        android:id="@+id/roadTile2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationY="-115dp"
        app:layout_constraintBottom_toTopOf="@id/safeTile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/roadTile"
        app:srcCompat="@drawable/road" />

    <ImageView
        android:id="@+id/safeTile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationY="-195dp"
        app:layout_constraintBottom_toTopOf="@id/riverTile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/roadTile2"
        app:srcCompat="@drawable/safe" />

    <ImageView
        android:id="@+id/riverTile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationY="-275dp"
        app:layout_constraintBottom_toTopOf="@id/riverTile2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/safeTile"
        app:srcCompat="@drawable/river" />

    <ImageView
        android:id="@+id/riverTile2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationY="-355dp"
        app:layout_constraintBottom_toTopOf="@id/riverTile3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/riverTile"
        app:srcCompat="@drawable/river" />

    <ImageView
        android:id="@+id/riverTile3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationY="-435dp"
        app:layout_constraintBottom_toTopOf="@id/safeTile2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/riverTile2"
        app:srcCompat="@drawable/river" />

    <ImageView
        android:id="@+id/safeTile2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationY="-515dp"
        app:layout_constraintBottom_toTopOf="@id/roadTile3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/riverTile3"
        app:srcCompat="@drawable/safe" />

    <ImageView
        android:id="@+id/roadTile3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationY="-595dp"
        app:layout_constraintBottom_toTopOf="@id/roadTile4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/safeTile2"
        app:srcCompat="@drawable/road" />

    <ImageView
        android:id="@+id/roadTile4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationY="-675dp"
        app:layout_constraintBottom_toTopOf="@id/roadTile5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/roadTile3"
        app:srcCompat="@drawable/road" />

    <ImageView
        android:id="@+id/roadTile5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationY="-755dp"
        app:layout_constraintBottom_toTopOf="@id/safeTile3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/roadTile4"
        app:srcCompat="@drawable/road" />

    <ImageView
        android:id="@+id/safeTile3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationY="-835dp"
        app:layout_constraintBottom_toTopOf="@id/riverTile4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/roadTile5"
        app:srcCompat="@drawable/safe" />

    <ImageView
        android:id="@+id/riverTile4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationY="-915dp"
        app:layout_constraintBottom_toTopOf="@id/riverTile5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/safeTile3"
        app:srcCompat="@drawable/river" />

    <ImageView
        android:id="@+id/riverTile5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationY="-995dp"
        app:layout_constraintBottom_toTopOf="@id/safeTile4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/riverTile4"
        app:srcCompat="@drawable/river" />

    <ImageView
        android:id="@+id/safeTile4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationY="-1075dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/riverTile5"

        app:srcCompat="@drawable/safe" />


    <TextView
        android:id="@+id/difficulty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/points"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.815"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/points"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="288dp"
        android:layout_marginBottom="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/name"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="288dp"
        android:layout_marginBottom="616dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/largeLog"
        android:layout_width="81dp"
        android:layout_height="41dp"
        android:src="@drawable/large"
        tools:layout_editor_absoluteX="325dp"
        tools:layout_editor_absoluteY="596dp" />

    <ImageView
        android:id="@+id/largeLog1"
        android:layout_width="81dp"
        android:layout_height="41dp"
        android:src="@drawable/large"
        tools:layout_editor_absoluteX="325dp"
        tools:layout_editor_absoluteY="250dp" />

    <ImageView
        android:id="@+id/largeLog2"
        android:layout_width="81dp"
        android:layout_height="41dp"
        android:src="@drawable/large"
        tools:layout_editor_absoluteX="150dp"
        tools:layout_editor_absoluteY="200dp" />

    <ImageView
        android:id="@+id/largeLog3"
        android:layout_width="81dp"
        android:layout_height="41dp"
        android:src="@drawable/large"
        tools:layout_editor_absoluteX="150dp"
        tools:layout_editor_absoluteY="200dp" />

    <ImageView
        android:id="@+id/largeLog4"
        android:layout_width="81dp"
        android:layout_height="41dp"
        android:src="@drawable/large"
        tools:layout_editor_absoluteX="150dp"
        tools:layout_editor_absoluteY="200dp" />

    <ImageView
        android:id="@+id/smallLog"
        android:layout_width="81dp"
        android:layout_height="41dp"
        android:src="@drawable/small"
        tools:layout_editor_absoluteX="350dp"
        tools:layout_editor_absoluteY="635dp" />

    <ImageView
        android:id="@+id/smallLog1"
        android:layout_width="81dp"
        android:layout_height="41dp"
        android:src="@drawable/small"
        tools:layout_editor_absoluteX="310dp"
        tools:layout_editor_absoluteY="300dp" />

    <ImageView
        android:id="@+id/smallLog2"
        android:layout_width="81dp"
        android:layout_height="41dp"
        android:src="@drawable/small"
        tools:layout_editor_absoluteX="350dp"
        tools:layout_editor_absoluteY="200dp" />

    <ImageView
        android:id="@+id/smallLog3"
        android:layout_width="81dp"
        android:layout_height="41dp"
        android:src="@drawable/small"
        tools:layout_editor_absoluteX="350dp"
        tools:layout_editor_absoluteY="200dp" />

    <ImageView
        android:id="@+id/smallLog4"
        android:layout_width="81dp"
        android:layout_height="41dp"
        android:src="@drawable/small"
        tools:layout_editor_absoluteX="350dp"
        tools:layout_editor_absoluteY="200dp" />

    <ImageView
        android:id="@+id/sprite"
        android:layout_width="115dp"
        android:layout_height="114dp"
        android:adjustViewBounds="true"
        android:contentDescription="the car"
        android:scaleType="centerCrop"

        android:scaleX=".5"
        android:scaleY=".5"
        android:translationY="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/beast_cartman" />

    <Button
        android:id="@+id/upButton"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="↑"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@id/downButton"
        app:layout_constraintStart_toEndOf="@id/leftButton"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="618dp" />

    <Button
        android:id="@+id/downButton"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="↓"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/leftButton"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="677dp" />

    <Button
        android:id="@+id/rightButton"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="→"
        android:textSize="23sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/downButton"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="647.5dp" />

    <Button
        android:id="@+id/leftButton"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="←"
        android:textSize="23sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="647.5dp" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="left"
        app:constraint_referenced_ids="goalTile" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="goalTile" />

    <ImageView
        android:id="@+id/bus1"
        android:layout_width="81dp"
        android:layout_height="41dp"
        android:src="@drawable/southparkbusresized"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="64dp" />

    <ImageView
        android:id="@+id/nyckids"
        android:layout_width="90dp"
        android:layout_height="28dp"
        app:srcCompat="@drawable/newyorkkidsresized"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="115dp" />

    <ImageView
        android:id="@+id/kenny1"
        android:layout_width="31dp"
        android:layout_height="33dp"
        app:srcCompat="@drawable/kennyresized"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="404dp" />

    <ImageView
        android:id="@+id/kenny2"
        android:layout_width="27dp"
        android:layout_height="37dp"
        app:srcCompat="@drawable/kennyresized"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="495dp" />

    <ImageView
        android:id="@+id/nyckids2"
        android:layout_width="64dp"
        android:layout_height="28dp"
        app:srcCompat="@drawable/newyorkkidsresized"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="452dp" />

    <TextView
        android:id="@+id/lives"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/difficulty"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.815"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>